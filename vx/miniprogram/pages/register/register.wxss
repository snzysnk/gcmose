/* miniprogram/pages/register/register.wxss */
.car {
    position: relative;
    top: 30rpx;
    display: flex;
    width: 600rpx;
    margin-left: 75rpx;
    height: 350rpx;
    justify-content: center;
    align-items: center;
    display: flex;
    border-radius: 20rpx;
}

.carImg {
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: .5;
}

.description {
    position: absolute;
    display: flex;
    align-items: center;
    justify-content: center;
}

.cuIcon-camera {
    font-size: 100rpx;
}

.register_info {
    width: 600rpx;
    padding-top: 50rpx;
    margin-left: 75rpx;
}

.cu-form-group .ttl {
    width: 6em;
}

.cu-form-group .inputDefault {
    text-align: center;
}

.cu-form-group picker .inputDefault {
    text-align: center;
}
